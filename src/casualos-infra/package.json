    },
    "dependencies": {
        "@pulumi/aws": "^6.0.0",
        "@pulumi/aws-apigateway": "^2.0.0",
        "@pulumi/aws-native": "^0.116.0",
        "@pulumi/awsx": "^2.0.2",
        "@pulumi/pulumi": "^3.113.0",
        "es-toolkit": "1.39.10",
        "commander": "12.0.0",
        "@casual-simulation/aux-common": "workspace:*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/casual-simulation/casualos.git"
    },
    "bugs": {
        "url": "https://github.com/casual-simulation/casualos/issues"
    },
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "/README.md",
        "/LICENSE.txt",
        "**/*.js",
        "**/*.js.map",
        "**/*.d.ts",
        "**/*.def"
