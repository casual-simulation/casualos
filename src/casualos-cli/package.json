    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "@casual-simulation/aux-common": "^3.3.4",
        "@casual-simulation/aux-records": "^3.3.4",
        "axios": "1.6.2",
        "commander": "12.0.0",
        "conf": "12.0.0",
        "open": "10.1.0",
        "prompts": "2.4.2"
    },
    "devDependencies": {
        "@types/prompts": "^2.4.9"
    },
    "files": [
