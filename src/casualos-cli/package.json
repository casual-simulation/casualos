{
    "name": "casualos",
    "version": "3.3.4",
    "description": "Command line interface for CasualOS.",
    "main": "./dist/index.js",
    "types": "index.d.ts",
    "module": "./dist/index.js",
    "bin": {
        "casualos": "./dist/cli.js"
    },
    "scripts": {
        "start": "node ./dist/cli.js",
        "dev": "npm run build && npm start",
        "build": "node ./script/build-cli.mjs",
